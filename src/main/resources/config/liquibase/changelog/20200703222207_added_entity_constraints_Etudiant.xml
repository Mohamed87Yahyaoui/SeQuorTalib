<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity Etudiant.
    -->
    <changeSet id="20200703222207-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="etablissement_id"
                                 baseTableName="etudiant"
                                 constraintName="fk_etudiant_etablissement_id"
                                 referencedColumnNames="id"
                                 referencedTableName="etablissement"/>

        <addForeignKeyConstraint baseColumnNames="id"
                                 baseTableName="etudiant"
                                 constraintName="fk_etudiant_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

        <addForeignKeyConstraint baseColumnNames="filiere_id"
                                 baseTableName="etudiant"
                                 constraintName="fk_etudiant_filiere_id"
                                 referencedColumnNames="id"
                                 referencedTableName="filiere"/>

        <addForeignKeyConstraint baseColumnNames="etudiant_id"
                                 baseTableName="etudiant_module"
                                 constraintName="fk_etudiant_module_etudiant_id"
                                 referencedColumnNames="id"
                                 referencedTableName="etudiant"/>

        <addForeignKeyConstraint baseColumnNames="module_id"
                                 baseTableName="etudiant_module"
                                 constraintName="fk_etudiant_module_module_id"
                                 referencedColumnNames="id"
                                 referencedTableName="module"/>
        
    </changeSet>
</databaseChangeLog>
